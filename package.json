    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "tesseract.js": "^7.0.0",
    "zod": "^3.24.1"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.14",
    "dotenv-cli": "^7.6.0",
    "eslint": "^9",
    "eslint-config-next": "^16.1.2",
